<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qishuai.app.intent04"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.qishuai.app.intent04.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.qishuai.app.intent04.OtherActivity" >
            <intent-filter>
                <action android:name="first" />

                <category android:name="ANDROID.INTENT.CATEGORY.DEFAULT" />

                <data
                    android:host="com.qishuai.intent04"
                    android:mimeType="com.qishuai.first/com"
                    android:path="/c/1"
                    android:port="8888"
                    android:scheme="http" >
                </data>
            </intent-filter>
        </activity>
    </application>

</manifest>